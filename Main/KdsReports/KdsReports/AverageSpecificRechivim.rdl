  <Code>function GetErech(byVal iKodRechiiv as integer,byVal dErech as Decimal)  As Decimal
        select  iKodRechiiv
	case  269, 129,126,118,117,1,115,114,113,112,78,77,76,54,33,30,28,26,7,1
		Return (dErech /60)
	case else
		 Return dErech 
        end select
end function

    Function IsExistIn(ByVal p1 As Integer, ByVal s1 As String) As Integer
      Dim s As String() = s1.Split(",")
        For Each item As String In s
            If item = p1.ToString() Then
                Return 1
            End If
        Next
        Return 0

    End Function
